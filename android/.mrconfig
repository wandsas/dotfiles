# ~/android/.mrconfig

[DEFAULT]
include =
	cat ~/.config/mr/mr-stow-plugin 2>/dev/null || true
	cat ~/.config/mr/mr-hooks 2>/dev/null || true
lib = . ~/.config/mr/mr-functions

[$HOME/work/android-container-scripts]
checkout = git clone git://github.com/google/android-emulator-container-scripts.git $MR_REPO
remotes = git_add_remote local rpi3:/srv/git/android-emulator-container-scripts.git
skip = ! ubuntu_on_ubu3

[$HOME/work/lxc-for-android]
checkout = git clone git://github.com/clondroid/lxc-for-Android-7.1.2.git $MR_REPO
skip = ! ubuntu_on_ubu3

